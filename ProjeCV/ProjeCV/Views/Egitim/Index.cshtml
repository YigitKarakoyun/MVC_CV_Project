@model ProjeCV.Models.Sinif.Class1
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}


<table class="table table-bordered">
    <thead>
        <tr>
            <th>ID</th>
            <th>BAŞLIK</th>
            <th>ALT BAŞLIK</th>
            <th>BÖLÜM</th>
            <th>GNO</th>
            <th>İŞLEMLER</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var x in Model.Deger3)
        {
            <tr>
                <td>@x.ID</td>
                <td>@x.TITLE</td>
                <td>@x.SUBTITLE</td>
                <td>@x.DEPARTMENT</td>
                <td>@x.GPA</td>
                <td>
                    <a href="/Egitim/EgitimSil/@x.ID" class="btn btn-danger">SİL</a>
                    <a href="/Egitim/EgitimSil/@x.ID" class="btn btn-success">GÜNCELLE</a>
                </td>

            </tr>
        }

    </tbody>
</table>

<a href="/Egitim/YeniEgitim/" class="btn btn-info">Yeni Eğitim Ekle</a>