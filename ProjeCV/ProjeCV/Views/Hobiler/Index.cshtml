@model ProjeCV.Models.Sinif.Class1
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}


<table class="table table-bordered">
    <thead>
        <tr>
            <th>ID</th>
            <th>HOBİLERİM</th>
            <th>İŞLEMLER</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var x in Model.Deger5)
        {
        <tr>
            <td>@x.ID</td>
            <td>@x.INTEREST</td>
            <td>
                @Html.ActionLink("Sil Butonu", "Sil", new { id = x.ID }, new { @class = "btn btn-warning", onclick = "return confirm('Silmek İstiyormusunuz?')" })
                <a href="/Hobiler/HobiGetir/@x.ID" class="btn btn-success">Güncelle</a>
            </td>
            

        </tr>
        }

    </tbody>
</table>

<a href="/Hobiler/YeniHobi/" class="btn btn-info">Yeni Hobi Ekle</a>